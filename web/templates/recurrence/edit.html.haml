.container
  .row
    .col.l6.offset-l3.s12
      .card.white.black-text
        .card-content
          %span.card-title Edit Recurring Transaction
          = render "form.html", account_map: @account_map, changeset: @changeset, action: recurrence_path(@conn, :update, @recurrence), conn: @conn, date: @recurrence.start_date
          = link "Back", to: page_path(@conn, :index)
